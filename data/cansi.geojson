var alminhas={"type": "FeatureCollection","name": "cansi","crs": { "type": "name", "properties": { "name": "urn:ogc:def:crs:OGC:1.3:CRS84" } },"features": [{ "type": "Feature", "properties": { "name": "Carrazeda de Ansiães, Rua Nossa Senhora dos Aflitos", "DESCRIÇÃO DO ORATÓRIO": "«(...) É um misto de alminhas e cruzeiro (...) composta em forma de um nicho. A cruz expõe o motivo principal de Cristo crucificado, mas pelas s diversas faces estão representados os símbolos da Paixão, como os cravos, a torquês, o martelo, etc. Sob os pés do crucifixo existe um cálice e abaixo um nicho decorado com umas alminhas de contornos naïf (...)», PEREIRA, 36", "FICHA INVENTÁRIO": "0036-FEC-BR", "FREGUESIA": "Carrazeda de Ansiães", "LAT": "41.244988", "LONG": "-7.299689", "OBS": "https://docs.google.com/document/d/e/2PACX-1vQxpVNiEqExLjKqBNvHXXnGFWqagCOweFPxYJfA18zuzcRNfLMxJC8qfKJLQvEO3jVsJ-gXNspSt7K4/pub", "PAINEL": "", "TIPOLOGIA": "nicho/cruz", "gx_media_links": "https://doc-14-58-mymaps.googleusercontent.com/untrusted/hostedimage/ohe0dbgcv9jn7mbp3tk1ghb69o/iohac4453is5ia9490u5eoj1ks/1693511033500/3evDZZ2F1ONbZ8Mo7WuI6QKm2vmYGVzF/07330417508546578562/5ACnmet7yB2J40uk121JbFY2iZ0WNUHCQvqVLwvdkE3--2hyvyIru8Oqqlm44Dq7Lu3m_4M7tIz3L8n9dTAvhj-Icihyli5OEztOdMlgEUosjXIxMRLfR3La0hmLabu9pQEOeky1ZW3TaHHGP3L6UarZ5hlldbr_zqekVHovedixX9ThGi6ee_hJWYhgX0w_zESUx5wM1CKA3HQHtD2LBHfPb5BiC9KbUsGu4ps5FjPfjnq7aNsc?session=0&fife" }, "geometry": { "type": "Point", "coordinates": [ -7.299689, 41.244988, 0.0 ] } },{ "type": "Feature", "properties": { "name": "Amedo, Rua do António", "DESCRIÇÃO DO ORATÓRIO": "Oratório em granito, tipo nicho, de vão retangular e cornija redonda saliente, rematado por uma cruz latina assente num plinto. Edícula de vão retangular e topo redondo.", "FICHA INVENTÁRIO": "0037-CAR-BR", "FREGUESIA": "Amedo", "LAT": "41.258535", "LONG": "-7.324413", "OBS": "https://docs.google.com/document/d/e/2PACX-1vTHAUl6p4F__NULzwu8NPDwZ_6hRGlNBQeADtUh414aqmy2nIk2K5A1_pysXIoLVT5NqPHrrxCIBZOI/pu